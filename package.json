{
  "name": "bgg-extension",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "yarn start",
    "prestart": "yarn tsc && yarn bundle",
    "start": "concurrently \"yarn web-ext:firefox\" \"yarn watch\"",
    "watch": "NODE_ENV=development tsc-watch --noClear --onSuccess \"yarn bundle\"",
    "bundle": "NODE_ENV=development rollup -c",
    "web-ext:firefox": "web-ext run --firefox-profile ./browser-profiles/firefox --profile-create-if-missing --keep-profile-changes --watch-file dist/bundle.js --start-url https://www.spelexperten.com/sallskapsspel/strategispel/root.html --start-url https://alphaspel.se/search/?query=root",
    "api:dev": "vercel dev"
  },
  "dependencies": {
    "bgg-xml-api-client": "^0.1.10",
    "web-ext": "^6.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/jest": "^27.4.1",
    "@vercel/node": "^1.14.1",
    "concurrently": "^7.1.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "rollup": "^2.70.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.4",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3"
  }
}
